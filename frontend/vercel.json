{
  "redirects": [
    {
      "has": [
        {
          "type": "host",
          "value": "moodify-ruddy.vercel.app"
        }
      ],
      "source": "/(.*)",
      "destination": "https://moodify-music.vercel.app/",
      "permanent": true
    }
  ],
   "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}