{
  "redirects": [
    {
      "has": [
        {
          "type": "host",
          "value": "moodify-ruddy.vercel.app"
        }
      ],
      "source": "/(.*)",
      "destination": "https://moodify-music.vercel.app/",
      "permanent": true
    }
  ],
   "rewrites": [
    {
      "source": "/((?!api/|sitemap.xml|robots.txt|manifest.webmanifest|favicon.ico|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}