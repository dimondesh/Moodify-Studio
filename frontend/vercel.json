{
  "redirects": [
    {
      "has": [
        {
          "type": "host",
          "value": "moodify-ruddy.vercel.app"
        }
      ],
      "source": "/(.*)",
      "destination": "https://moodify-music.vercel.app/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/|assets/|_next/|static/|sw.js|robots.txt|sitemap.xml|manifest.webmanifest|favicon.ico|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}